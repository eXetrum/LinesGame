        <!-- Row # 0 -->
        <Button Command="{Binding Path=ClickCommand}" CommandParameter="0" Content="0"  Grid.Row="0" Grid.Column="0" Name="Cell_0_0" >
            <Button.Style>
                <Style TargetType="Button">
                    <Setter Property="OverridesDefaultStyle" Value="True"/>
                </Style>
            </Button.Style>
            <Button.Template>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Ellipse StrokeThickness="3" Stroke="{Binding Path=Game.Field[0].Selected, Mode=OneWay, Converter={converters:SelectToColor}, ConverterParameter='0'}">
                            <Ellipse.Fill>
                                <RadialGradientBrush>
                                    <GradientStop Offset="0" Color="Azure" />
                                    <GradientStop Offset="1" Color="{Binding Path=Game.Field[0].CellColor, NotifyOnTargetUpdated=True }" />
                                    <RadialGradientBrush.Transform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleY="0.65" />
                                        </TransformGroup>
                                    </RadialGradientBrush.Transform>
                                </RadialGradientBrush>
                            </Ellipse.Fill>
                        </Ellipse>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Button.Template>
            <Button.Triggers>
                <EventTrigger RoutedEvent="Binding.TargetUpdated">
                    <BeginStoryboard>
                        <Storyboard AutoReverse="False">
                            <DoubleAnimation Completed="Timeline_Completed" Storyboard.TargetName="Cell_0_0" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="00:00:01" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Button.Triggers>
        </Button>

