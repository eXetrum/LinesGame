        <GroupBox Grid.Column="9" Header="Статистика игры" HorizontalAlignment="Left" Margin="60,0,-229,0" Width="219" Grid.RowSpan="7" Height="350" VerticalAlignment="Top">
            <Grid Margin="0,0,-2,0">
                <Label Content="Время:" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="82"/>
                <Label Content="{Binding Path=Game.ElapsedTime, StringFormat={}{0:hh\:mm}}" HorizontalAlignment="Right" Margin="0,10,0,0" VerticalAlignment="Top" Width="65" Height="26" />
                <Label Content="Сделано ходов:" HorizontalAlignment="Left" Margin="0,50,0,0" VerticalAlignment="Top" Width="93"/>
                <Label Content="{Binding Path=Game._GameScore.ElapsedMoves}" HorizontalAlignment="Right" Margin="0,50,0,0" VerticalAlignment="Top" Width="65" Height="26"/>
                <Label Content="Максимальное комбо:" HorizontalAlignment="Left" Margin="0,90,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.632,3.115"/>
                <Label Content="{Binding Path=Game._GameScore.MaxCombo}" HorizontalAlignment="Left" Margin="144,90,0,0" VerticalAlignment="Top" Width="65" Height="26"/>
                
                <Label Content="Очки:" HorizontalAlignment="Left" Margin="0,130,0,0" VerticalAlignment="Top" Width="52"/>                
                <Label Content="{Binding Path=Game._GameScore.Score}" HorizontalAlignment="Right" Margin="0,130,0,0" VerticalAlignment="Top" Width="65" Height="26"/>
                <Label Content="Тип игры:" HorizontalAlignment="Left" Margin="-1,250,0,0" VerticalAlignment="Top"/>
                <Label Content="{Binding Source={x:Static p:Settings.Default}, Path=GameType, Mode=OneTime, Converter={converters:GameTypeToLabel}, ConverterParameter='0'}" HorizontalAlignment="Left" Margin="63,250,0,0" VerticalAlignment="Top" Width="146"/>
                <Label Content="{Binding Source={x:Static p:Settings.Default}, Path=GameType, Mode=OneTime, Converter={converters:GameTypeToLabel}, ConverterParameter='1'}" HorizontalAlignment="Left" Margin="0,290,0,0" VerticalAlignment="Top" Width="117"/>
                <Label Content="{Binding Path=Game.Remained, Mode=OneWay}" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="65" Margin="140,290,0,0" Height="24"/>
                
            </Grid>
        </GroupBox>
